<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<HBox xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.ontrack.overview.calendar.CalendarCellController">
   <children>
      <Label fx:id="cellDateLabel" maxWidth="40.0" minWidth="40.0" prefWidth="40.0" text="Day">
         <HBox.margin>
            <Insets bottom="10.0" left="5.0" top="10.0" />
         </HBox.margin>
         <font>
            <Font size="18.0" />
         </font></Label>
      <VBox fx:id="taskListVbox" spacing="5.0">
         <HBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </HBox.margin></VBox>
   </children>
</HBox>
