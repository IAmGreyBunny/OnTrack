<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>

<HBox fx:id="encompassingHbox" alignment="CENTER_LEFT" focusTraversable="true" minHeight="20.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.ontrack.overview.calendar.TaskItemCellController">
   <children>
      <Label fx:id="taskNameLabel" text="Task Name">
         <HBox.margin>
            <Insets left="5.0" />
         </HBox.margin>
      </Label>
      <CheckBox fx:id="doneCheckBox" mnemonicParsing="false" visible="false">
         <HBox.margin>
            <Insets left="10.0" right="10.0" />
         </HBox.margin>
      </CheckBox>
   </children>
</HBox>
